  const isPalindrome = function(str) {
      let arr = Array.from(str);
      let check = arr.reverse();
      check = check.join('')
      if (str === check) {
          return true;
      } else {
          return false;
        }
        
    // your code here - don't forget to return a boolean!
  };
  
